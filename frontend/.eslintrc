{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "commonjs": true,
      "es6": true 
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-unused-vars": "warn"
    },
    "plugins": [
        "react"
    ]
}